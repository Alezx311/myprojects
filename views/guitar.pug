<!DOCTYPE html>
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(http-equiv="X-UA-Compatible", content="ie=edge")
    title Guitar Page
    link(rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.8.0/css/bulma.min.css")
    script(defer src="https://use.fontawesome.com/releases/v5.3.1/js/all.js")
    link(rel="stylesheet", href='stylesheets/guitar.css')
    script(src='/javascripts/lib/tone.js')
    script(src='/javascripts/lib/zepto.js')
    script(src='/javascripts/lib/tone-instruments.js')
    script(src='/javascripts/lib/p5.js')
    script(src='/javascripts/lib/p5.sound.js')
    script(src='/javascripts/guitar_database.js')
    
  body
    section.section
      .container
        #guitarDrawElement
      .container
        ul#pianoDrawElement
          li(data-note='C4' class='piano-white-key') D
            div.piano-black-key(data-note="C#4") R
          li(data-note='D4' class='piano-white-key') F
            div.piano-black-key(data-note="D#4") T
          li(data-note='E4' class='piano-white-key') G
          li(data-note='F4' class='piano-white-key') H
            div.piano-black-key(data-note="F#4") U
          li(data-note='G4' class='piano-white-key') J
            div.piano-black-key(data-note="G#4") I
          li(data-note='A4' class='piano-white-key') K
            div.piano-black-key(data-note="A#4") O
          li(data-note='B4' class='piano-white-key') L
    
    
    
    script(src='/javascripts/guitarscript.js', type="text/javascript")